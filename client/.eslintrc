{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-return-await": 0,
    "no-underscore-dangle": 0,
    "prettier/prettier": ["error", {
      "printWidth": 100,
      "singleQuote": true,
      "trailingComma": "es5",
      "classMethodsUseThis": false
    }],
    "import/prefer-default-export": 0
  },
  "env": {
    "node": true,
    "jest": true
  },
  "plugins": [
    "prettier"
  ]
}
